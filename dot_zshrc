export PATH=$HOME/bin:$PATH
export ZSH="$HOME/.oh-my-zsh"
export EDITOR=nano
ZSH_THEME="agnoster"
CASE_SENSITIVE="true"
COMPLETION_WAITING_DOTS="true"
plugins=(zsh-syntax-highlighting zsh-autosuggestions colored-man-pages chezmoi)

source $ZSH/oh-my-zsh.sh

cd $HOME

function gamsrd() { "$HOME/bin/gam/gam" "$@" ; }
alias gam="/home/david/bin/gamadv-xtd3/gam"
